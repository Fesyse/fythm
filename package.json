{
  "name": "fythm-discord-bot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "db:studio": "drizzle-kit studio --config=./src/db/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=./src/db/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@angular-devkit/schematics-cli": "^18.0.6",
    "@discord-nestjs/common": "^5.2.12",
    "@discord-nestjs/core": "^5.3.14",
    "@discord-nestjs/schematics": "^0.4.14",
    "@discordjs/voice": "^0.17.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@vercel/postgres": "^0.9.0",
    "@xata.io/client": "^0.29.5",
    "@xata.io/drizzle": "^0.0.22",
    "discord-player": "^6.6.10",
    "discord.js": "^14.15.3",
    "drizzle-orm": "^0.31.2",
    "image-dimensions": "^2.3.0",
    "nestjs-dynamic-providers": "^0.3.4",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "ytdl-core": "^4.11.5",
    "ytubes": "^1.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "drizzle-kit": "^0.22.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
